{
  "name": "workouttracker",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "start:fresh": "rm -rf node_modules/.cache .expo/cache $TMPDIR/metro-* $TMPDIR/haste-* 2>/dev/null; expo start --clear --port 8081 --localhost",
    "clean": "rm -rf node_modules/.cache .expo/cache $TMPDIR/metro-* $TMPDIR/haste-* 2>/dev/null; echo Done. Run npm run start:fresh to start with a clean bundle.",
    "reload": "lsof -ti :8082 | xargs kill -9 2>/dev/null; expo start --clear --port 8082",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "patch-package",
    "test:e2e": "~/.maestro/bin/maestro test .maestro/",
    "test:e2e:single": "~/.maestro/bin/maestro test"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^7.2.2",
    "@react-navigation/native": "^7.0.13",
    "@react-navigation/native-stack": "^7.2.0",
    "@supabase/supabase-js": "^2.95.3",
    "dayjs": "^1.11.13",
    "expo": "^53.0.27",
    "expo-apple-authentication": "~7.2.4",
    "expo-av": "~15.1.7",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "~7.1.5",
    "expo-crypto": "~14.1.5",
    "expo-device": "~7.1.4",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-keep-awake": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-sensors": "~14.1.4",
    "expo-sharing": "~13.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "openai": "^4.104.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "^0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-shadow-2": "^7.1.2",
    "react-native-svg": "15.11.2",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "babel-preset-expo": "~13.0.0",
    "jest": "~29.7.0",
    "patch-package": "^8.0.1",
    "ts-jest": "^29.4.6",
    "typescript": "~5.8.3"
  },
  "private": true
}
