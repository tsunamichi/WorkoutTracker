{
  "expo": {
    "name": "WorkoutTracker",
    "slug": "WorkoutTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tsunamichi.workouttracker",
      "buildNumber": "96",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSSupportsLiveActivities": true,
        "NSSupportsLiveActivitiesFrequentUpdates": true,
        "NSCameraUsageDescription": "Allow access to take progress photos for your weekly check-in.",
        "NSPhotoLibraryUsageDescription": "Allow access to select progress photos for your weekly check-in.",
        "NSPhotoLibraryAddUsageDescription": "Allow saving progress photos to your library.",
        "NSMotionUsageDescription": "Motion is used so the celebration animation responds to how you hold your device."
      },
      "backgroundColor": "#0D0D0D"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tsunamichi.workouttracker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cc9457fe-c6f5-45ab-9110-40b941491c17"
      }
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-system-ui"
    ],
    "owner": "tsunamichi",
    "backgroundColor": "#0D0D0D"
  }
}